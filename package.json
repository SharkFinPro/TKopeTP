{
  "name": "tkopetp",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.mjs",
  "scripts": {
    "start": "node .",
    "build": "npm run build-app && npm run build-server && npm run build-pkg",
    "build-noCompression": "npm run build-app && npm run build-server && npm run build-pkg-noCompression",
    "build-app": "npx babel src/assets/appScriptsDev --out-dir src/assets/app/scripts --presets react-app/prod",
    "build-server": "npx webpack",
    "build-pkg": "npx pkg . -C Brotli --output \"bin/Trading Post\"",
    "build-pkg-noCompression": "npx pkg . --output \"bin/Trading Post\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "excel4node": "^1.8.0",
    "express": "^4.18.1",
    "sqlite3": "^5.1.2"
  },
  "pkg": {
    "assets": [
      "node_modules/compression/**/*",
      "node_modules/express/**/*",
      "node_modules/sqlite3/**/*",
      "src/assets/app/**/*",
      "src/assets/adminPanel/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "bin": "src/bin/server.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "pkg": "^5.8.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
